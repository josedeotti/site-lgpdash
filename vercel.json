{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://backend.lgpdash.pro/api/:path*"
    }
  ],
  "redirects": [
    {
      "source": "/login",
      "destination": "https://backend.lgpdash.pro/login",
      "permanent": false
    },
    {
      "source": "/app/:path*",
      "destination": "https://backend.lgpdash.pro/app/:path*",
      "permanent": false
    },
    {
      "source": "/dashboard/:path*",
      "destination": "https://backend.lgpdash.pro/dashboard/:path*",
      "permanent": false
    },
    {
      "source": "/painel/:path*",
      "destination": "https://backend.lgpdash.pro/painel/:path*",
      "permanent": false
    }
  ]
}